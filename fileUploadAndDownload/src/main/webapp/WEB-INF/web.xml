<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>uploadHandlerServlet</servlet-name>
        <servlet-class>com.zql.servlet.UploadHandlerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadHandlerServlet</servlet-name>
        <url-pattern>/servlet/uploadHandlerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>listFileServlet</servlet-name>
        <servlet-class>com.zql.servlet.ListFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>listFileServlet</servlet-name>
        <url-pattern>/servlet/listFileServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>downloadServlet</servlet-name>
        <servlet-class>com.zql.servlet.DownloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>downloadServlet</servlet-name>
        <url-pattern>/servlet/downloadServlet</url-pattern>
    </servlet-mapping>
</web-app>
